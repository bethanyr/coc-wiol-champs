<%iplace = 1 %>
<% if (runners.size > 0) %>
  <% runners.each do |result| %>
    <% if (result.entryclass == run_class[0]) %>
    <tr>
     <td valign=top >&nbsp<% if result.non_compete %><%="nc" %><% else %> <%=iplace.to_s %> <% end %></td>
                   <td align=left><%=result.firstname%> <%=result.surname%></td>
                   <td><%=result.school%></td>
                   <td><% if result.time1 != nil %><%=time_to_value(result.classifier1, result.time1, result.float_time1) %><% end %></td>
                   <% if !result.non_compete %>
                    <td><% if result.day1_score != nil %><%=result.day1_score.round(2) %><% end %></td>
                    <td><% if result.day2_score != nil %><%=result.day2_score.round(2) %><% end %></td>
                    <td align=left><% if result.day1_score != nil and result.day2_score and result.total %><%=result.total.round(2).to_s%><% end %></td>
                  <% end %>
                  <%iplace += 1%>
                   
                 </tr>
             
             <% end %>
    <% end %>
<% else %>
  <tr><td colspan="2">No entries</td></tr>
<% end %>
