            <tr>
              <td valign=top >&nbsp<%=iplace.to_s %></td>
              <td align=left><b><%=result.firstname%> <%=result.surname%></b><br>
                <%=result.school%><br>
                Day1: <%=result.day1_score.round(3) %> Day2: <%=result.day2_score.round(3) %>
              </td>
              <td align=left><%=result.total.round(2).to_s%></td>
