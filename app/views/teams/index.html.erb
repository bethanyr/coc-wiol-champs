<p id="notice"><%= notice %></p>


<table border = 1 width = 100% style="border-collapse: collapse; order-spacing: 0;
    margin: 0; padding: 0;">
  <tr style="background-color:#002868">
    <th align="center" colspan="4"><font color="white">Team Results</font></th>
  </tr>
  <tr style="background-color:LightBlue">
    <th align="center" width="33%"></th>
    <th align="center" width="33%"></th>
    <th align="center" width="33%"></th>
    
  </tr>
  <tr>
    <td valign=top>
      <table>
        <tr><th colspan="2">Varsity School</th></tr>
        <%= render 'team_list', :teams => @isv_school %>
        <tr><th colspan="2">Varsity Club</th></tr>
        <%= render 'team_list', :teams => @isv_club %>
        <tr><th colspan="2">College Varsity</th></tr>
        <%= render 'team_list', :teams => @icv_school %>
        
      </table>
    </td>
    <td valign=top>
      <table>
        <tr><th colspan="2">JV School</th></tr>
        <%= render 'team_list', :teams => @isjv_school %>
        <tr><th colspan="2">Varsity JROTC</th></tr>
        <%= render 'team_list', :teams => @isv_jrotc %>
         <tr><th colspan="2">JV Club</th></tr>
        <%= render 'team_list', :teams => @isjv_club %>
        
      </table>
    </td>
    <td valign=top>
      <table>
        <tr><th colspan="2">Intermediate School</th></tr>
        <%= render 'team_list', :teams => @isi_school %>
        <tr><th colspan="2">Intermediate Club</th></tr>
        <%= render 'team_list', :teams => @isi_club %>
        <tr><th colspan="2">JV JROTC</th></tr>
        <%= render 'team_list', :teams => @isjv_jrotc %>
      </table>
    </td>
    
  </tr>
</table>